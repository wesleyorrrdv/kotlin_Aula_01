fun main(){
     val carro = Carro()
     carro.cor = "Carro Cinza"
    carro.tudo()
    carro.andarCarro()
    println(carro.cor)
}
class Carro(){
    var cor: String = " "
    
    fun andarCarro(){
        println("Andando")
    }
    fun pararCarro(){
        println("Parar")
    }
    fun reCarro(){
        println("RÃ©")
    }
    fun tudo(){
        this.andarCarro()
        this.reCarro()
        this.pararCarro()
    }
    
    
}

