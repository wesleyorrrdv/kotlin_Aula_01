/** Operadores Relacionais
 * 
 *  == Operador de Igual
 * < Operador de Menor
 * > Operador de Maior
 * <= Operador de Menor ou Ingual
 * >= Operador de Maior ou Ingual
 * != Operador de Diferente
 *  
 */
fun main() {
   var valor1 = 20
    var valor2 = 10
    
    println(valor1 == valor2)
    println(valor1 < valor2)
    println(valor1 > valor2)
    println(valor1 <= valor2)
    println(valor1 >= valor2)
    println(valor1 != valor2)
}
